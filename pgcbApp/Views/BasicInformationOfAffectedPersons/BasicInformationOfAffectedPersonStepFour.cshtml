    <script>
        $(document).ready(function () {
            //  console.log("ready");
            $("#yes").click(function () {
                // console.log("Click");
                $("#year").show();
                $("#money").show();
                $("#IsAffectedLandUnderShare").show();
            });
            $("#no").click(function () {
                //   console.log("Click");
                $("#year").hide();
                $("#money").hide();
                $("#IsAffectedLandUnderShare").hide();
            });


            $("#FamilyStatus").change(function () {
                //alert("ok");

                var name = $("#FamilyStatus").val();
                console.log(name);
                if (name == "Year deficit") {
                    $("#YearDeficit").show();
                } else {
                    $("#YearDeficit").hide();
                }
            });

        });

    </script>
}


